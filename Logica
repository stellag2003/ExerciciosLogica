       double carro1= 30000;
       double carro2= 40000;
       double carro3= 30000;
       double vendas_totais= carro1+carro2+carro3;
       double taxaCarro1= carro1 * 5/100;
       double taxaCarro2= carro2 * 5/100;
       double taxaCarro3= carro3 * 5/100;
       
      
       Console.WriteLine($"Voce vendeu 3 carros");
       Console.WriteLine($"Voce vendeu R${(vendas_totais +300*3) + taxaCarro1 + taxaCarro2 + taxaCarro3}");
       Console.WriteLine($"O seu salario ao final desse mes sera: {salario + 300*3 + taxaCarro1 + taxaCarro2 + taxaCarro3 }");
       
       
