         1)
         double votosBrancos=10000;
         double votosNulos=10000;
         double total_eleitores=50000;
         double votosValidos= 30000;
         double porcentagemBrancos= votosBrancos/total_eleitores;
         double porcentagemNulos= votosNulos/total_eleitores;
         double porcentagemValidos= votosValidos/total_eleitores;
        
        
       
        Console.WriteLine($"Total de eleitores:{total_eleitores}");
      
        Console.WriteLine($"Numero de votos brancos:{votosBrancos}");
        
        Console.WriteLine($"Numero de votos nulos:{votosNulos}");
     
        Console.WriteLine($"Numero de votos validos:{votosValidos}");
        
        Console.WriteLine("Percentual de votos:");
        
        Console.WriteLine($"Votos brancos:{porcentagemBrancos}%");
        
        Console.WriteLine($"Votos nulos: {porcentagemNulos}%");
        
        Console.WriteLine($"Votos validos:{porcentagemValidos}%");
        
        
